<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="520.0" prefWidth="940.0" xmlns="http://javafx.com/javafx/8.0.261" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Telas.FXMLLoginController">
   <center>
      <VBox alignment="CENTER" fillWidth="false" prefHeight="200.0" prefWidth="100.0" spacing="50.0" BorderPane.alignment="CENTER">
         <children>
            <JFXTextField fx:id="tb_login" focusColor="#009a0a" labelFloat="true" prefHeight="20.0" prefWidth="256.0" promptText="Login" unFocusColor="BLACK" />
            <JFXPasswordField fx:id="tb_senha" focusColor="#009a0a" prefHeight="20.0" prefWidth="256.0" promptText="Senha" unFocusColor="BLACK" />
            <Button fx:id="btn_entrar" mnemonicParsing="false" onAction="#ClickEntrar" prefWidth="96.0" text="Entrar" />
         </children>
      </VBox>
   </center>
</BorderPane>
